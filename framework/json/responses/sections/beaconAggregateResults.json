                },
                "name": {
                    "description": "Name of this category",
                    "example": "Genders",
                    "type": "string"
                }
            },
            "required": [
                "id",
                "distribution"
            ],
            "type": "object"
        }
    },
    "$schema": "https://json-schema.org/draft/2020-12/schema",
